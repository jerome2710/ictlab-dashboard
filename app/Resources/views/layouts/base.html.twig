<!DOCTYPE html>
<!--[if lt IE 8]><html class="lt-ie10 lt-ie9 lt-ie8" lang="{{ app.request.locale }}"><![endif]-->
<!--[if IE 8]><html class="lt-ie10 lt-ie9" lang="{{ app.request.locale }}"><![endif]-->
<!--[if IE 9]><html class="lt-ie10" lang="{{ app.request.locale }}"><![endif]-->
<!--[if gt IE 9]><!--><html lang="{{ app.request.locale }}"><!--<![endif]-->
	<head>
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta charset="utf-8">
		<title>CHIBB {% if page_title is defined %} - {{ page_title }}{% endif %}</title>
		<meta content="CHIBB Dashboard" name="">
		<meta content="" name="keywords">
		<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<link rel="apple-touch-icon" sizes="180x180" href="/assets/images/favicons/apple-touch-icon.png">
		<link rel="icon" type="image/png" href="/assets/images/favicons/favicon-32x32.png" sizes="32x32">
		<link rel="icon" type="image/png" href="/assets/images/favicons/favicon-16x16.png" sizes="16x16">
		<link rel="manifest" href="/assets/images/favicons/manifest.json">
		<link rel="mask-icon" href="/assets/images/favicons/safari-pinned-tab.svg" color="#232323">
		<link rel="shortcut icon" href="/assets/images/favicons/favicon.ico">
		<meta name="msapplication-config" content="/assets/images/favicons/browserconfig.xml">
		<meta name="theme-color" content="#ffffff">
		<script>document.documentElement.className += ' js';var app={},chibb={};</script>
		<link rel="dns-prefetch" href="//fonts.googleapis.com">
		<link rel="dns-prefetch" href="//google-analytics.com">

		{% include 'inc/favicons.html.twig' only %}
		<link href="//fonts.googleapis.com/css?family=Roboto:400,500,700,800,900" rel="stylesheet">

		{% block stylesheets %}
			{% if app.environment != 'dev' %}
				{% stylesheets 'assets/css/styles.css' filter='cssrewrite' %}
				<link id="styles" rel="stylesheet" href="{{ asset_url }}">
				{% endstylesheets %}
			{% else %}
				<link id="styles" rel="stylesheet" href="/assets/css/styles.css">
			{% endif %}
		{% endblock %}
	</head>
	<body>
		{% include 'inc/header.html.twig' %}

		{% include 'inc/sidebar.html.twig' %}

		{% include 'inc/messages.html.twig' %}

		<div id="page-content-wrapper">
			<div class="container-fluid {{ page_class|default('default') }}">
				<div class="row header">
					<div class="col-lg-12">
						<h1 class="title">CHIBB - {{ page_title|default('') }}</h1>
					</div>
				</div>
				{% block body %}{% endblock %}
			</div>
		</div>

		{% include 'inc/footer.html.twig' %}

		{% block javascripts %}
			<script src="/assets/js/build/build.js"></script>
			<script type="text/javascript">svg4everybody();</script>
		{% endblock %}
	</body>
</html>